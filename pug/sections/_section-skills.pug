//- Includes
include ../elements/_ui-counter-type-1.pug

// Skills
section.section.novi-background.section-98.section-md-110.context-dark(style="background: #191919 url(images/backgrounds/background-02-481x360.png);")
  .container-fluid
    .row
      .col-md-8.offset-md-2.col-lg-12.offset-lg-0
        .row
          .col-md-6.col-lg-3.offset-lg-0
            +counter-type-1("Templates", "font-weight-bold text-blue-gray", {
              "dataTo": "58249",
              "dataSpeed": "3000",
              "attr": "text-uppercase font-weight-bold text-spacing-60 offset-top-20"
            }, "divider bg-default", "h1")
          .col-md-6.col-lg-3.offset-top-66.offset-md-top-0
            +counter-type-1("Happy Clients", "font-weight-bold text-blue-gray", {
              "dataTo": "246",
              "dataSpeed": "2500",
              "preffix": "K",
              "attr": "text-uppercase font-weight-bold text-spacing-60 offset-top-20"
            }, "divider bg-default", "h1")
          .col-md-6.col-lg-3.offset-top-66.offset-lg-top-0
            +counter-type-1("Hours of Coding", "font-weight-bold text-blue-gray", {
              "dataTo": "1200",
              "dataSpeed": "1500",
              "attr": "text-uppercase font-weight-bold text-spacing-60 offset-top-20"
            }, "divider bg-default", "h1")
          .col-md-6.col-lg-3.offset-top-66.offset-lg-top-0
            +counter-type-1("Lines of Code", "font-weight-bold text-blue-gray", {
              "dataTo": "834",
              "dataSpeed": "1300",
              "preffix": "K",
              "attr": "text-uppercase font-weight-bold text-spacing-60 offset-top-20"
            }, "divider bg-default", "h1")
