extends  ../_skeleton

//- Includes
include ../elements/_ui-breadcrumb-classic
include ../elements/_ui-rd-map

//-Title
block title
  title= cfg.nav.pages.submenu.contactUsV3.title

//- Page
block page
  // Page Header
  header.page-head
    //- RD Navbar
    +rd-navbar-default(
      'rd-navbar-default rd-navbar-light',
      {
        "data-lg-auto-height": "true",
        "data-md-layout": 'rd-navbar-fixed',
        "data-lg-layout": 'rd-navbar-static',
        "data-lg-stick-up": 'true'
      },
      cfg.brand.logo,
      cfg.nav.pages,
      cfg.nav
    )

  +breadcrumb-classic(
    cfg.nav.pages.submenu.contactUsV3.title,
    cfg.nav.pages.submenu.contactUsV3.icon,
    {
      0: {
        "title": cfg.nav.home.title,
        "href": cfg.nav.home.href
      },
      1: {
        "title": cfg.nav.pages.title,
        "href": cfg.nav.pages.href
      },
      2: {
        "title": cfg.nav.pages.submenu.contactUsV3.title,
        "href": cfg.nav.pages.submenu.contactUsV3.href
      }
    })

  include ../sections/_section-contact-info
  include ../sections/_section-get-in-touch-variant-2
  include ../sections/_section-google-map

  // Page Footer
  include ../sections/_section-footer-default
