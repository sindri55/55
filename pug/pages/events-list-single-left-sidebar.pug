extends  ../_skeleton
//- Includes
include ../elements/_ui-breadcrumb-classic
//-Title
block title
  title= cfg.nav.blog.submenu.events.title
//- Page
block page
  // Page Header
  header.page-head
    //- RD Navbar
    +rd-navbar-default(
      'rd-navbar-default rd-navbar-light',
      {
        "data-lg-auto-height": "true",
        "data-md-layout": 'rd-navbar-fixed',
        "data-lg-layout": 'rd-navbar-static',
        "data-lg-stick-up": 'true'
      },
      cfg.brand.logo,
      cfg.nav.blog,
      cfg.nav
    )

  +breadcrumb-classic(
    cfg.nav.blog.submenu.events.title,
    cfg.nav.blog.submenu.events.icon,
    {
      0: {
        "title": cfg.nav.home.title,
        "href": cfg.nav.home.href
      },
      1: {
        "title": cfg.nav.blog.title,
        "href": cfg.nav.blog.href
      },
      2: {
        "title": cfg.nav.blog.submenu.events.title,
        "href": cfg.nav.blog.submenu.events.href
      },
      3: {
        "title": cfg.nav.blog.submenu.events.submenu.singleEvent.title,
        "href": cfg.nav.blog.submenu.events.submenu.singleEvent.href
      },
      4: {
        "title": cfg.nav.blog.submenu.events.submenu.singleEvent.submenu.leftSidebar.title,
        "href": cfg.nav.blog.submenu.events.submenu.singleEvent.submenu.leftSidebar.href
      }
    })

  section.section.novi-background.section-98.section-sm-110
    .container
      .row.justify-content-sm-center
        .col-lg-4.col-xl-3
          include ../sections/_section-blog-sidebar-variant-5
        .col-lg-8.offset-top-66.offset-lg-top-0
          .inset-lg-left-20
            include ../sections/_section-event-list-single-right

  // Page Footer
  include ../sections/_section-footer-default
