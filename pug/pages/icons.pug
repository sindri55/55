  // Page Header
  header.page-head
    //- RD Navbar
    +rd-navbar-default(
      'rd-navbar-default rd-navbar-light',
      {
        "data-lg-auto-height": "true",
        "data-md-layout": 'rd-navbar-fixed',
        "data-lg-layout": 'rd-navbar-static',
        "data-lg-stick-up": 'true'
      },
      cfg.brand.logo,
      cfg.nav.components,
      cfg.nav
    )


  +breadcrumb-classic(
    cfg.nav.components.submenu.toolkitComponents.submenu.icons.title,
    "icon-lg " + cfg.nav.components.submenu.toolkitComponents.submenu.icons.icon,
    {
      0: {
        "title": cfg.nav.home.title,
        "href": cfg.nav.home.href
      },
      1: {
        "title": cfg.nav.components.title,
        "href": cfg.nav.components.href
      },
      2: {
        "title": cfg.nav.components.submenu.toolkitComponents.submenu.icons.title,
        "href": cfg.nav.components.submenu.toolkitComponents.submenu.icons.href
