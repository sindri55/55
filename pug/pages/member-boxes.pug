                  },
                  {
                    "icon": "fa fa-google-plus",
                    "href": "#"
                  }
                ], "icon-xs icon-circle icon-darker-filled", "list-inline-xs")
            .col-md-6.col-xl-3.offset-top-66.offset-xl-top-0
              +box-member("images/users/user-ronald-oswald-270x270.jpg", "Ronald Oswald", "Developer", "#")
                +widget-list-social([
                  {
                    "icon": "fa fa-facebook",
                    "href": "#"
                  },
                  {
                    "icon": "fa fa-twitter",
                    "href": "#"
                  },
                  {
                    "icon": "fa fa-google-plus",
                    "href": "#"
                  }
                ], "icon-xs icon-circle icon-darker-filled", "list-inline-xs")

  // Member BoxesMember Boxes Type 3
  section.section.novi-background.section-66
    .container
      h1 Member Boxes Type 3
      .row.justify-content-sm-center
        .col-sm-10.col-md-5
          +member-block-type-5(
