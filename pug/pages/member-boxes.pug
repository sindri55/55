extends  ../_skeleton

//- Includes
include ../elements/_ui-breadcrumb-classic
include ../elements/_ui-member-block
include ../elements/_widget-list-social
include ../elements/_ui-box-member
include ../elements/_ui-member-block-type-5
include ../elements/_ui-progress-bar-linear
include ../elements/_ui-member-block-type-4
include ../elements/_ui-member-block-type-3

//-Title
block title
  title= cfg.nav.components.submenu.toolkitComponents.submenu.memberBoxes.title

//- Page
block page
  // Page Header
  header.page-head
    //- RD Navbar
    +rd-navbar-default(
    'rd-navbar-default rd-navbar-light',
    {
      "data-lg-auto-height": "true",
      "data-md-layout": 'rd-navbar-fixed',
      "data-lg-layout": 'rd-navbar-static',
      "data-lg-stick-up": 'true'
    },
    cfg.brand.logo,
    cfg.nav.components,
    cfg.nav
    )

  +breadcrumb-classic(
  cfg.nav.components.submenu.toolkitComponents.submenu.memberBoxes.title,
  "icon-lg " + cfg.nav.components.submenu.toolkitComponents.submenu.memberBoxes.icon,
  {
    0: {
      "title": cfg.nav.home.title,
      "href": cfg.nav.home.href
    },
    1: {
      "title": cfg.nav.components.title,
      "href": cfg.nav.components.href
    },
    2: {
      "title": cfg.nav.components.submenu.toolkitComponents.submenu.memberBoxes.title,
      "href": cfg.nav.components.submenu.toolkitComponents.submenu.memberBoxes.href
    }
  })

  // Member BoxesMember Boxes Type 1
  section.section.novi-background.section-66.setion-md-top-100
    .container
      h1 Member Boxes Type 1
      hr.divider.bg-mantis
      .row.justify-content-sm-center.offset-top-66
        .col-md-8.col-xl-12
          .row
            .col-md-6.col-xl-3
              +member-block(
              {"title": "John Doe", "href": "#"},
              {"src": "images/users/user-john-doe-140x140.jpg", "width": 140, "height": 140, "alt": ""},
              "Software Developer",
              [
                {"icon": "fa fa-facebook", "href": "#"},
                {"icon": "fa fa-twitter", "href": "#"},
                {"icon": "fa fa-google-plus", "href": "#"}
              ]
              )

            .col-md-6.col-xl-3.offset-top-66.offset-md-top-0
              +member-block(
              {"title": "July Mao", "href": "#"},
              {"src": "images/users/user-july-mao-140x140.jpg", "width": 140, "height": 140, "alt": ""},
              "Support Operator",
              [
                {"icon": "fa fa-facebook", "href": "#"},
                {"icon": "fa fa-twitter", "href": "#"},
                {"icon": "fa fa-google-plus", "href": "#"}
              ]
              )

            .col-md-6.col-xl-3.offset-top-66.offset-xl-top-0
              +member-block(
              {"title": "Bernard Show", "href": "#"},
              {"src": "images/users/user-bernard-show-140x140.jpg", "width": 140, "height": 140, "alt": ""},
              "Creative Director",
              [
                {"icon": "fa fa-facebook", "href": "#"},
                {"icon": "fa fa-twitter", "href": "#"},
                {"icon": "fa fa-google-plus", "href": "#"}
              ]
              )

            .col-md-6.col-xl-3.offset-top-66.offset-xl-top-0
              +member-block(
                {"title": "Ronald Oswald", "href": "#"},
                {"src": "images/users/user-ronald-oswald-140x140.jpg", "width": 140, "height": 140, "alt": ""},
                "Founder, Sales",
                [
                  {"icon": "fa fa-facebook", "href": "#"},
                  {"icon": "fa fa-twitter", "href": "#"},
                  {"icon": "fa fa-google-plus", "href": "#"}
                ]
              )

  // Member BoxesMember Boxes Type 2
  section.section.novi-background.section-66
    .container
      h1 Member Boxes Type 2
      hr.divider.bg-mantis
      .row.justify-content-sm-center.offset-md-top-66
        .col-md-10.col-xl-12
          .row
            .col-md-6.col-xl-3.offset-top-66.offset-sm-top-0
              +box-member("images/users/user-john-doe-270x270.jpg", "John Doe", "Founder", "#")
                +widget-list-social([
                  {
                    "icon": "fa fa-facebook",
                    "href": "#"
                  },
                  {
                    "icon": "fa fa-twitter",
                    "href": "#"
                  },
                  {
                    "icon": "fa fa-google-plus",
                    "href": "#"
                  }
